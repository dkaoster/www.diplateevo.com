{
  "name": "www.diplateevo.com",
  "version": "0.0.0",
  "description": "Diplateevo, based on Svelte and Sveltekit",
  "type": "module",
  "main": "index.js",
  "engines": {
    "npm": "^6",
    "node": "^14"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "prebuild": "node genImageSizes.js",
    "build": "svelte-kit build",
    "postbuild": "mv build/404/index.html build/404.html && npx svelte-sitemap --domain https://diplateevo.com --ignore 404.html --trailing-slashes",
    "preview": "svelte-kit preview",
    "lint": "eslint --ignore-path .gitignore --ext .js,.cjs,.svelte .",
    "clean": "rm -rf build && rm -rf .svelte-kit && find . -name \"*@*.*\" | xargs rm && rm -f content/images-manifest.json"
  },
  "author": "Daniel Kao",
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.21",
    "@sveltejs/kit": "^1.0.0-next.201",
    "archieml": "^0.4.2",
    "autoprefixer": "^10.4.2",
    "d3": "^7.3.0",
    "dompurify": "^2.3.5",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "feed": "^4.2.1",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.9",
    "node-sass": "^7.0.1",
    "postcss": "^8.2.10",
    "regl": "^2.1.0",
    "regl-camera": "^2.1.1",
    "rollup": "^2.18.1",
    "rollup-plugin-generate-image-sizes": "0.0.18",
    "svelte": "^3.44.3",
    "svelte-preprocess": "^4.10.1",
    "svelte-sitemap": "^2.2.0"
  }
}
